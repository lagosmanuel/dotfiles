[user]
    name = lagosmanuel
    email = lagosmanual@gmail.com
    signingkey = ssh-ed25519 AAAAC3NzaC1lZDI1NTE5AAAAIJjqOLtoE4U3DcWFrKihUliOJcO5/TSWfcTJ84zh4NSk
[core]
    editor = vim
    excludesfile = ~/.gitignore
    whitespace = fix,blank-at-eol,blank-at-eof,space-before-tab,tab-in-indent,cr-at-eol
    autocrlf = false
[init]
    defaultBranch = main
[web]
    browser = firefox
[credential]
    helper = cache --timeout=3600
[pull]
    rebase = true
[commit]
    gpgsign = true
[alias]
    tree = log --oneline --graph --decorate --all
[merge]
    tool = vimdiff
[gpg]
	format = ssh
